<%- include('partials/headerfornotsigned')%>
  <nav id="searchNav">
    <ul>
      <li><button class="newt" id="userBtn">Users</button></li>
      <li>&nbsp; / &nbsp; </li>
      <li><button class="newt" id="quesBtn">Questions</button></li>
    </ul>
  </nav>

  <div class="userBtn xt">
    <%if(user.length===0){%>
      <section>
        <div id="userDiv">
          <div>
            <h1>No User Found! ðŸ˜ž</h1>
          </div>
        </div>
      </section>

      <%}else{%>
        <section>
          <div id="userDiv">
            <% user.forEach(function(se){%>
              <div id="user">
                <hr />
                <!-- <i class="fas fa-user-circle fa-2x"></i> -->
                <img src="/image/<%=se._id%>" alt="profile photo" class="profileIcon">
                <a href="/profile/<%=se._id%>">
                  <h4>
                    <%=se.detail.FullName%>
                  </h4>
                </a>
                <hr />
              </div>
              <br>
              <%});%>
          </div>
        </section>
        <%}%>
  </div>


  <div class="quesBlock xt">
    <%if(questions.length===0){%>
      <section id="querySec">
        <div id="queryDiv">
          <div>
            <h1>No Questions Found! ðŸ˜ž</h1>
          </div>
        </div>
      </section>
      <%}else{%>
        <section id="querySec">
          <div id="queryDiv">
            <% questions.forEach(function(se){%>
              <div class="quesDiv">
                <a href="/question/<%=se._id%>">
                  <p>Q. <%=se.ques%>
                  </p>
                </a>
              </div>
              <br>
              <%});%>
          </div>
        </section>
        <%}%>
  </div>
  <%- include('partials/Footerfornotsigned')%>